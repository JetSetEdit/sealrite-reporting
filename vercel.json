{
  "buildCommand": "cd dashboard && npm install && npm run build",
  "outputDirectory": "dashboard/build",
  "installCommand": "npm install",
  "functions": {
    "api/instagram/kpis.js": {
      "maxDuration": 60
    },
    "api/export-report.js": {
      "maxDuration": 120
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 